	private static String toString(TimeUnit unit) {
		switch (unit) {
		case NANOSECONDS:
			return "ns"; //$NON-NLS-1$
		case MICROSECONDS:
			return "Âµs"; //$NON-NLS-1$
		case MILLISECONDS:
			return "ms"; //$NON-NLS-1$
		case SECONDS:
			return "s"; //$NON-NLS-1$
		case MINUTES:
			return "m"; //$NON-NLS-1$
		case HOURS:
			return "h"; //$NON-NLS-1$
		case DAYS:
			return "d"; //$NON-NLS-1$
		default:
			return ""; //$NON-NLS-1$
		}
	}

