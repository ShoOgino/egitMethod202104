		@Override
		public long getLastModified() {
			if (fileMode == FileMode.SYMLINK) {
				try {
					File file = asFile();
					if (file != null)
						return FS.DETECTED.lastModified(file);
					return 0;
				} catch (IOException e) {
					return 0;
				}
			}
			return rsrc.getLocalTimeStamp();
		}

