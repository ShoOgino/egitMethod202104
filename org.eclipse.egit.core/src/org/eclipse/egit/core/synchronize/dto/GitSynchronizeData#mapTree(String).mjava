	private Tree mapTree(String rev) throws IOException {
		if (rev.startsWith(Constants.R_TAGS)) {
			Tag tag = repo.mapTag(rev);
			if (tag != null) {
				Commit commit = repo.mapCommit(tag.getObjId());
				if (commit != null)
					return commit.getTree();
			}
			return null;
		} else
			return repo.mapTree(rev);
	}

