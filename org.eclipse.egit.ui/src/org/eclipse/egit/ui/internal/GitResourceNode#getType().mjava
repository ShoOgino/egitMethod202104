	public String getType() {
		if (entry instanceof Tree)
			return ITypedElement.FOLDER_TYPE;
		else {
			if (entry != null) {
				String name = entry.getName();
				if (name != null) {
					int index = name.lastIndexOf('.');
					if (index == -1)
						return ""; //$NON-NLS-1$
					if (index == (name.length() - 1))
						return ""; //$NON-NLS-1$
					return name.substring(index + 1);
				}
				return "";
			} else
				return "";
		}
	}

