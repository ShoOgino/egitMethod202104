	@Override
	public boolean equals(Object obj) {
		if (obj == this)
			return true;

		if (obj == null)
			return false;

		if (obj.getClass() != getClass())
			return false;

		GitModelCacheTree objTree = (GitModelCacheTree) obj;
		return objTree.getLocation().equals(getLocation())
					&& objTree.getBaseId().equals(getBaseId());
	}

