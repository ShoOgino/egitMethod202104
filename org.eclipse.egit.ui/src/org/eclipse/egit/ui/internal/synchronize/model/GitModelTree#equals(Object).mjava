	@Override
	public boolean equals(Object obj) {
		if (obj == this)
			return true;

		if (obj == null)
			return false;

		if (obj.getClass() != getClass())
			return false;

		GitModelTree objTree = (GitModelTree) obj;
		return objTree.location.equals(location)
				&& objTree.baseCommit.equals(baseCommit);
	}

