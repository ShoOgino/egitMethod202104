	StorageSizeFieldEditor(String name, String labelText, Composite parent,
			int min, int max) {
		init(name, labelText);
		setTextLimit(10);
		setEmptyStringAllowed(false);
		setErrorMessage(JFaceResources
				.getString("IntegerFieldEditor.errorMessage")); //$NON-NLS-1$
		createControl(parent);
		minValidValue = min;
		maxValidValue = max;
	}

