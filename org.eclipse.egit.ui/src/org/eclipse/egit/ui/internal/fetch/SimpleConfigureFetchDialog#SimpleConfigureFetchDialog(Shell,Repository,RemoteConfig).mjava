	/**
	 * @param shell
	 * @param repository
	 * @param config
	 */
	private SimpleConfigureFetchDialog(Shell shell, Repository repository,
			RemoteConfig config) {
		super(shell);
		setHelpAvailable(false);
		setShellStyle(getShellStyle() | SWT.SHELL_TRIM);
		this.repository = repository;
		this.config = config;
	}

