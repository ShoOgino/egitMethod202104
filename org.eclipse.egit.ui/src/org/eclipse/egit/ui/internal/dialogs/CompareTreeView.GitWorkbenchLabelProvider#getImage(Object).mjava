		@Override
		public Image getImage(Object element) {
			Image superImage = myProvider.getImage(element);
			if (superImage == null)
				return DELETED;
			if (element instanceof IFile) {
				if (equalIds.contains(new Path(repositoryMapping
						.getRepoRelativePath((IFile) element)))) {
					return SAME_CONTENT;
				}
			}
			return superImage;
		}

