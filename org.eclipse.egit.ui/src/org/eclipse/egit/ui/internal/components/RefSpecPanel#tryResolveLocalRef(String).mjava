	private ObjectId tryResolveLocalRef(final String ref) {
		if (!isValidRefExpression(ref))
			return null;
		try {
			return localDb.resolve(ref);
		} catch (final IOException e) {
			Activator.logError(
					"I/O error occurred during resolving expression: " //$NON-NLS-1$
							+ ref, e);
			return null;
		}
	}

