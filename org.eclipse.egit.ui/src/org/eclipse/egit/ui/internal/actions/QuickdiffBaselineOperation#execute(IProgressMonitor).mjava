	public void execute(IProgressMonitor monitor) throws CoreException {
		try {
			GitQuickDiffProvider.setBaselineReference(repository, baseline);
		} catch (IOException e) {
			Activator
					.logError(
							UIText.QuickdiffBaselineOperation_baseline,
							e);
		}
	}

